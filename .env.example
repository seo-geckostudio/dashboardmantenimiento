# Database Configuration
DB_PASS=your_secure_password_here
DB_ROOT_PASS=your_root_password_here

# User/Group IDs for file permissions
PUID=1000
PGID=1000

# JWT Secret for Dashboard Authentication
JWT_SECRET=your_jwt_secret_key_here

# MCP Authentication Token for Trae Integration
MCP_AUTH_TOKEN=your_mcp_auth_token_here

# Scan Configuration
SCAN_ROOTS=/home/*/public_html:/var/www/*
EXCLUDE_DIRS=node_modules:vendor:cache:.git

# Application Environment
APP_ENV=prod
APP_DEBUG=false